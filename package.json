  "description": "- Development\r ![Build badge](https://s3-extras-bucket-coredocs-workflow-api-dev.s3-ap-southeast-2.amazonaws.com/badges/badge-build-action-build.svg)\r ![Unit Tests badge](https://s3-extras-bucket-coredocs-workflow-api-dev.s3-ap-southeast-2.amazonaws.com/badges/badge-unit-test-action-build.svg)\r ![E2E Tests badge](https://s3-extras-bucket-coredocs-workflow-api-dev.s3-ap-southeast-2.amazonaws.com/badges/badge-e2e-test-action-build.svg)\r ![Deployment status badge](https://s3-extras-bucket-coredocs-workflow-api-dev.s3-ap-southeast-2.amazonaws.com/badges/badge-deployment-status.svg)",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "prettier": "prettier \"**/*.{js,ts}\" --check",
    "prettier:fix": "npm run prettier -- --write",
    "start": "NODE_ENV=development ts-node -r tsconfig-paths/register src/main.ts",
    "start:docker": "docker-compose up --build",
    "watch": "NODE_ENV=development nodemon",
    "watch:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "NODE_ENV=production node -r ./tsconfig-paths-bootstrap.js dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint:fix": "npm run lint -- --fix",
    "format": "npm run prettier:fix && npm run lint:fix",
    "test": "npm run test:e2e && npm run test:unit",
    "test:e2e": "jest --config ./test/jest-e2e.json --verbose --runInBand --passWithNoTests --testTimeout=120000 --forceExit",
    "test:unit": "jest --config ./test/jest-unit.json --verbose --runInBand --passWithNoTests --testTimeout=120000 --forceExit",
    "test:watch:e2e": "npm run test:e2e -- --watch",
    "test:watch:unit": "npm run test:unit -- --watch",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config/typeorm.config.ts",
    "typeorm:create": "npm run typeorm migration:create -- -n",
    "typeorm:generate": "npm run typeorm migration:generate -- -n",
    "typeorm:run": "npm run typeorm migration:run",
    "typeorm:revert": "npm run typeorm migration:revert",
    "generate:resource": "nest g resource"
  },
